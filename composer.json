{
    "name":         "becklyn/route-tree-bundle",
    "description":  "This bundle provides a simple implementation of a tree built from routing configuration.",
    "type":         "symfony-bundle",
    "homepage":     "https://github.com/Becklyn/BecklynRouteTreeBundle",
    "license":      "BSD-3-Clause",
    "authors": [{
        "name":     "Becklyn Studios",
        "homepage": "http://becklyn.com/"
    }],
    "require": {
        "php": "^7.1",
        "knplabs/knp-menu": "^2.3",
        "knplabs/knp-menu-bundle": "^2.2",
        "symfony/cache": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/routing": "^4.2",
        "symfony/security-core": "^4.2",
        "symfony/translation": "^4.2"
    },
    "require-dev": {
        "becklyn/php-cs": "^1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "twig/twig": "^2.9"
    },
    "suggest": {
        "twig/twig": "To use the route_tree_menu() twig function.",
        "sensio/framework-extra-bundle": "To automatically infer from @Security and @IsGranted annotations."
    },
    "autoload": {
        "psr-4": { "Becklyn\\RouteTreeBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Becklyn\\RouteTreeBundle\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
